{
  "version": 2,
  "builds": [
    {
      "src": "textbook/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/textbook/build/$1"
    }
  ]
}
